pipeline {

agent any

stages {

   stage ('GIT Checkout'){

       steps {

           git changelog: false, poll: false, url: 'https://github.com/AntoninaRozhok/Py_script'

       }

   }

  

   stage('build') {

 steps {

   sh 'pip install -r requirements.txt'

 }

}

   stage ('Test'){

       steps {
        
       echo 'Testing using pytest'

           sh 'python robot'

       }

   }

}

}
