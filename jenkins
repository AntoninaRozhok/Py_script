pipeline {

agent any

stages {

   stage ('GIT Checkout'){

       steps {

           git branch: 'main', url: 'https://github.com/AntoninaRozhok/Py_script/blob/main/robot'

       }

   }

  

   stage('build') {

 steps {

   sh 'pip install -r requirements.txt'

 }

}

   stage ('Test'){

       steps {
        
       echo 'Testing using robot'

           sh 'python PycharmProjects/pythonProject/robot.robot'

       }

   }

}

}
